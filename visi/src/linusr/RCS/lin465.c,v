#include <stdio.h>
#include "line.h"

/*-----------------------------*/
/* קשקןה ףנעבקן‏מןך ימזןעםבדיי */
/* ף נעוהקבעיפולרמשם ףפיעבמיום */
/* ץ‏בףפכב כעבמב              */
/*-----------------------------*/
help ( from, to, text )
int     from;           /* קועטמסס חעבמידב (מןםוע ףפעןכי מב כעבמו) */
int     to;             /* מיצמסס חעבמידב */
char    **text;         /* ףנעבקן‏משך פוכףפ */
{
	register i ;

	er_scr(from, to);

	if(text) {
		for( i=from; *text; i++) {
			cp_set( i, 0, TXT);
			w_str( *text++ );   /* נןכבתבפר פוכףפ מב כעבמו */
		}
	}
}
@


