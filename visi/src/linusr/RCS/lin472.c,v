#include <stdio.h>
#include "line.h"


int     cvt_sp(line, cod, mod, str)
/*-----------------------------------*/
/* χχοδ στςολι πο δχοκξονυ υλαϊατεμΰ */
/*-----------------------------------*/
LINE *line;
kbcod cod;
char *mod;
char *str;
{
	register char **spp;

	spp = (char **)line->varl;
	if(*mod == 'r') strcpy(*spp, str);
	else            strcpy(str, *spp);
	return(TRUE);
}

