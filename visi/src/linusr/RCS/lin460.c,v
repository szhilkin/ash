#include <stdio.h>
#include "line.h"

/*---------------------------*/
/* קשקןה ףןןג‎ומיס ןג ןיגכו */
/*---------------------------*/
static  int errflg ;    /* זלבח: ףןןג‎ומיך ןג ןיגכו מו גשלן */

w_emsg ( str )
char    *str ;          /* פוכףפ ףןןג‎ומיס ןג ןיגכו */
{
	if( *str ) {    /* וףלי בעחץםומפ קשתןקב מונץףפבס ףפעןכב */
		errflg = 1;
		cp_set(-1, 0, ERR);
		bell();
		printf(" ?!! %s ", str);
	} else {
		if(errflg) {
			errflg = 0;
			cp_set(-1, 0, TXT ) ;
			er_eol();
		}
	}
}
@


