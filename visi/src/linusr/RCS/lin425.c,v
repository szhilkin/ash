#include <stdio.h>
#include <ctype.h>
#include "line.h"
#include "line0.h"

cvt_lh(line, cod, mod, str)
/*------------------------*/
/* קשקןה מבתקבמיס כלבקיי */
/*------------------------*/
LINE    *line;
int       cod;
char     *mod;
char     *str;
{
	/* מבךפי י נןהףפבקיפר מבתקבמיו כלבקיי
	 * נן וו ףםשףלןקןםץ (לןחי‏וףכןםץ) כןהץ
	 * מבנעיםוע ":NL" תבםומסופףס מב "RETURN"
	 */
	extern   unsigned strlen();
	register char   *s;
	extern   KBL    kbl[];  /* פבגלידב ףקסתי כןהןק י מבתקבמיך כלבקי */
	register KBL   *kblp;
		kbcod   codh;   /* כןה, הלס כןפןעןחן מבךפי מבתקבמיו */

	if( *mod == 'w' ) {
		s = line->varl;
		/* הלס פןחן וףפר םבכעןףש ק "line.h" */
		codh = tocod0(s[1]);
		codh |= tocod1(s[2]);

		for(kblp=kbl; kblp->t_key!=0; kblp++) {
			if(kblp->t_cod == codh) {
				strcpy(str, kblp->t_knm);
				goto ret;
			}
		}
		strcpy(str, &line->varl[1]);
ret:
/***** סקמבס ‏וץס...
		for(s=str+strlen(str), s--; s > str && *s; s--) {
			if(isspace(*s)) *s = '\0';
		}
 *****/
		;
	}
	return( TRUE ) ;        /* ןיגןכ גשפר מו םןצופ... */
}
@


